//  Divisibilidad de C entre A y B

#include <stdio.h>

	int divisibilidada(int a, int c){
		if(c%a==0) return 1;
		else
		return 0;
	}
		
	int divisibilidadb(int b, int c)	{
		if(c%b==0) return 1;
		else
		return 0;
	}
int main(void){	
	int a=0, b=0, c=0;
	printf("Indica el valor de A:  \n");
	scanf("%d", &a);
	printf("Indica el valor de B:  \n");
	scanf("%d", &b);
	printf("Indica el valor de C:  \n");
	scanf("%d", &c);
	
	if (divisibilidada(a, c))
	printf("%d es divisible entre %d  \n", c, a);
	else 
	printf("%d no es divisible entre %d  \n", c, a);
	
	if (divisibilidadb(b, c))
	printf("%d es divisible entre %d  \n", c, b);
	else 
	printf("%d no es divisible entre %d  \n", c, b);
	
return 0;		
	
}
