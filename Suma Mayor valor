//  Divisibilidad

#include <stdio.h>

int main(void) {
	int A, B, i;
	int M=0, total=0;
	printf("Indica el valor de A:  ");
	scanf("%d", &A);
	printf("\nIndica el valor de B:  ");
	scanf("%d", &B);
	
	if (A>B) M=A; else M=B;
	for(i=1; i<=M; i++)
	{
		total=total+i;
		printf("%d+", i);
	}
printf("\nSumatoria total: %d", total);

return 0;
}	
